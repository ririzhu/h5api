    $(window).scroll(function () {
        //滚动轴
        var CTop = document.documentElement.scrollTop || document.body.scrollTop;
//        var floorone=$(".home-standard-layout").eq(0).offset().top;
        var floorone=450;
        //当滚动轴到达楼层一时，左菜单栏显示
        if (CTop >= 300) {
            $("#indexleft").show(300);
        } else {
            $("#indexleft").hide(300);
        }
        //滚动楼层对应切换左侧楼层导航
        var le = $(".floor-left-index").length;
        var arr = [];
        for (var s = 0; s < le; s++) {
            var nums = $(".floor-left-index").eq(s).offset().top;
            arr.push(nums);
        }

        var scrTop = $(window).scrollTop();
        for (var w = 0; w < arr.length; w++) {
            var cc = arr[w + 1] || 1111111111;
            if (scrTop >= arr[w]*1+200 && scrTop <= cc) {
                if (arr[w + 1] < 0) {
                    w = w + 1;
                }
                $("#indexleft li ").removeClass("gettonva");
                $("#indexleft li").eq(w+1).addClass("gettonva");
            }else if(scrTop > arr[w]*1-100){
                $("#indexleft li ").removeClass("gettonva");
                $("#indexleft li").eq(w).addClass("gettonva");
            }
        }
    });

    $("#scrollTop").hover(function () {
        $("#scrollTop").css("background","rgb(173, 172, 172)");
    },function () {
        $("#scrollTop").css("background","#999");
    });
    reset_position();
    $(window).resize(function(){
        reset_position();
    });

    //左侧悬浮窗返回顶部事件
    function goTop() {
        $(window).scrollTop(0);
    }
</script>
